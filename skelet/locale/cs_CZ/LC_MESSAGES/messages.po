#: ../atk14/src/atk14/atk14_locale.inc:37
msgid "atk14.date_format"
msgstr "j.n.Y"

#: ../atk14/src/atk14/atk14_locale.inc:56
msgid "atk14.parse_date_pattern"
msgstr "/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4})$/"

#: ../atk14/src/atk14/atk14_locale.inc:90
msgid "atk14.datetime_format"
msgstr "j.n.Y H:i"

#: ../atk14/src/atk14/atk14_locale.inc:103
msgid "atk14.parse_datetime_pattern"
msgstr "/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4}) (?<hours>[0-9]{2}):(?<minutes>[0-9]{2})$/"

#: ../atk14/src/atk14/atk14_locale.inc:116
msgid "atk14.datetime_with_seconds_format"
msgstr "j.n.Y H:i:s"

#: ../atk14/src/atk14/atk14_locale.inc:128
msgid "atk14.parse_datetime_with_seconds_pattern"
msgstr "/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4}) (?<hours>[0-9]{2}):(?<minutes>[0-9]{2}):(?<seconds>[0-9]{2})$/"

