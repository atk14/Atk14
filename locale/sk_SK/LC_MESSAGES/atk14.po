#: ../src/atk14/atk14_locale.php:70
msgid "atk14.date_format"
msgstr "j.n.Y"

#: ../src/atk14/atk14_locale.php:90
msgid "atk14.parse_date_pattern"
msgstr "/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4})$/"

#: ../src/atk14/atk14_locale.php:124
msgid "atk14.datetime_format"
msgstr "j.n.Y H:i"

#: ../src/atk14/atk14_locale.php:137
msgid "atk14.parse_datetime_pattern"
msgstr ""
"/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4}) (?"
"<hours>[0-9]{2}):(?<minutes>[0-9]{2})$/"

#: ../src/atk14/atk14_locale.php:150
msgid "atk14.datetime_with_seconds_format"
msgstr "j.n.Y H:i:s"

#: ../src/atk14/atk14_locale.php:162
msgid "atk14.parse_datetime_with_seconds_pattern"
msgstr ""
"/^(?<day>[0-9]{1,2})\\.(?<month>[0-9]{1,2})\\.(?<year>[0-9]{4}) (?"
"<hours>[0-9]{2}):(?<minutes>[0-9]{2}):(?<seconds>[0-9]{2})$/"

#: ../src/atk14/atk14_locale.php:179
